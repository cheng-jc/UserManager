<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.cjc.view.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainFrame</servlet-name>
        <servlet-class>com.cjc.view.MainFrame</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginDealwithServlet</servlet-name>
        <servlet-class>com.cjc.controller.LoginDealwithServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginDealwithServlet</servlet-name>
        <url-pattern>/LoginDealwithServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainFrame</servlet-name>
        <url-pattern>/MainFrame</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>com.cjc.view.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.cjc.session.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/Servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.cjc.session.Servlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ManagerUsers</servlet-name>
        <servlet-class>com.cjc.view.ManagerUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManagerUsers</servlet-name>
        <url-pattern>/ManagerUsers</url-pattern>
    </servlet-mapping>
</web-app>